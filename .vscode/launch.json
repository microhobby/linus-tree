{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "name": "Kernel KDGB",
            "request": "launch",
            "program": "/home/castello/linus-tree/seadog-gaia/kernel/rpi/artifacts/bcm2711-rpi-4b/vmlinux",
            "cwd": "${workspaceFolder}",
            "symbolLoadInfo": {
                "loadAll": false,
                "exceptionList": ""
            },
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "text": "set arch aarch64"
                },
                {
                    "text": "target remote localhost:${config:kerneldev.kgdb_port}"
                },
            ],
            "preLaunchTask": "${command:embeddedLinuxDev.breakKernel}"
        },
        {
            "type": "crash",
            "request": "launch",
            "name": "Run Crash Utility",
            "crash": "/home/castello/tmp/crash/crash",
            "vmlinux": "/home/castello/linus-tree/seadog-gaia/kernel/rpi/artifacts/bcm2711-rpi-4b/vmlinux",
            "vmcore": "/home/castello/linus-tree/mount/var/log/vmcore"
        }
    ],
    "compounds": []
}
