{
    "kerneldev.experimental.newDtsEngine": true,
    "devicetree.bindings": [
        "${workspaceFolder}/Documentation/devicetree/bindings"
    ],
    "ctags.languages": [
        "Kconfig",
        "Make"
    ],
    "terminal.integrated.defaultProfile.linux": "bash",
    "files.associations": {
        "Dockerfile*": "dockerfile",
        "Containerfile*": "dockerfile",
        "docker-compose*": "yaml",
        "*.scr": "shellscript",
        "*defconfig*": "kconfig",
        "*.build": "powershell",
        "*.run": "powershell",
        "*COMMIT_EDITMSG*": "git-commit",
        ".wslconfig": "ini",
        "*.defs": "makefile",
        "*.ld": "linkerscript",
        "pid_namespace.h": "c",
        "mm.h": "c",
        "bug.h": "c",
        "nsproxy.h": "c",
        "idr.h": "c",
        "ns_common.h": "c",
        "workqueue.h": "c",
        "threads.h": "c"
    },
    "editor.tabSize": 8,
    "indentRainbow.ignoreErrorLanguages": [
        "c",
        "kconfig",
        "defconfig",
        "dts"
    ],
    "checkpatch.diagnosticLevel": "Warning",
    "C_Cpp.errorSquiggles": "disabled",
    "kerneldev.ssh_login": "seadog",
    "kerneldev.ssh_psswd": "seadog",
    "kerneldev.ssh_ip": "192.168.0.53",
    "kerneldev.kgdb_port": "6061",
    "kerneldev.serial_port": "6060",
    "kerneldev.serial_baudRate": "921600",
    "kerneldev.serial_dev": "ttyUSB0",
    "kerneldev.breakBySysrq": true
}
