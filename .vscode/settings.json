{
    "ctags.languages": [
        "Kconfig"
    ],
    "kerneldev.useDocker": false,
    "scm.showHistoryGraph": false,
    "devicetree.bindings": [
        "${workspaceFolder}/Documentation/devicetree/bindings"
    ],
    "kerneldev.experimental.newDtsEngine": true,
    "C_Cpp.intelliSenseCachePath": "${workspaceFolder}/.vscode/",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "C_Cpp.enhancedColorization": "enabled",
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.parameterNames.hideLeadingUnderscores": false,
    "C_Cpp.inlayHints.parameterNames.suppressWhenArgumentContainsName": false,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.showSpace": true,
    "files.associations": {
        "Dockerfile*": "dockerfile",
        "Containerfile*": "dockerfile",
        "docker-compose*": "yaml",
        "*.scr": "shellscript",
        "*defconfig*": "kconfig",
        "*.build": "powershell",
        "*.run": "powershell",
        "*COMMIT_EDITMSG*": "git-commit",
        ".wslconfig": "ini",
        "*.defs": "makefile",
        "*.ld": "linkerscript",
        "fb.h": "c",
        "kgdb.h": "c",
        "notifier.h": "c",
        "srcu.h": "c",
        "rcupdate.h": "c",
        "rcu_segcblist.h": "c",
        "workqueue.h": "c",
        "timer.h": "c",
        "stringify.h": "c",
        "ktime.h": "c",
        "jiffies.h": "c",
        "module.h": "c",
        "elf.h": "c",
        "sched.h": "c",
        "seccomp.h": "c",
        "unistd.h": "c",
        "compiler_types.h": "c",
        "fbcon.h": "c",
        "irq.h": "c",
        "vt_kern.h": "c",
        "kd.h": "c",
        "types.h": "c",
        "selection.h": "c",
        "string.h": "c",
        "fs.h": "c",
        "slab.h": "c",
        "uaccess.h": "c",
        "crc32.h": "c",
        "interrupt.h": "c",
        "init.h": "c",
        "console.h": "c",
        "fcntl.h": "c"
    }
}
